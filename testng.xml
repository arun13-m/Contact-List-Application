<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
<listeners>
<listener class-name="utility.Listener"></listener>
</listeners>

  <test thread-count="5" name="Chrome Test">
  <parameter name="browser" value="chrome"></parameter>
    <classes>
      <class name="tests.TC_001_SignUpTest"/>
      <class name="tests.TC_002_LoginTest"/>
      <class name="tests.TC_003_ContactAdditionTest"/> 
      <class name="tests.TC_004_ContactDisplayTest"/> 
      <class name="tests.TC_005_ContactEditingTest"/> 
      <class name="tests.TC_006_ContactDeletingTest"/>
      <class name="tests.TC_007_FinalLogoutTest"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="Edge Test">
  <parameter name="browser" value="edge"></parameter>
    <classes>
      <class name="tests.TC_001_SignUpTest"/>
      <class name="tests.TC_002_LoginTest"/> 
      <class name="tests.TC_003_ContactAdditionTest"/> 
      <class name="tests.TC_004_ContactDisplayTest"/> 
      <class name="tests.TC_005_ContactEditingTest"/> 
      <class name="tests.TC_006_ContactDeletingTest"/> 
      <class name="tests.TC_007_FinalLogoutTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
