<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="classes" name="Suite">
<listeners>
<listener class-name="utility.Listener"></listener>
</listeners>

  <test thread-count="5" parallel="classes" name="Chrome Test">
  <parameter name="browser" value="chrome"></parameter>
    <classes>
      <class name="tests.TC_005_ContactEditingTest"/>
      <class name="tests.TC_006_ContactDeletingTest"/>
      <class name="tests.TC_001_SignUpTest"/>
      <class name="tests.TC_007_FinalLogoutTest"/>
      <class name="tests.TC_003_ContactAdditionTest"/>
      <class name="tests.TC_002_LoginTest"/>
      <class name="tests.TC_004_ContactDisplayTest"/>
    </classes>
  </test> <!-- Test -->
  
  
  <test thread-count="5" parallel="classes" name="Edge Test">
  <parameter name="browser" value="Edge"></parameter>
    <classes>
      <class name="tests.TC_005_ContactEditingTest"/>
      <class name="tests.TC_006_ContactDeletingTest"/>
      <class name="tests.TC_001_SignUpTest"/>
      <class name="tests.TC_007_FinalLogoutTest"/>
      <class name="tests.TC_003_ContactAdditionTest"/>
      <class name="tests.TC_002_LoginTest"/>
      <class name="tests.TC_004_ContactDisplayTest"/>
    </classes>
  </test> <!-- Test -->
  
</suite> <!-- Suite -->
